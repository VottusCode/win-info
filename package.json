{
	"name": "active-win",
	"version": "4.0.0",
	"description": "Get metadata about the active window (title, id, bounds, owner, etc). Works on macOS, Linux, Windows.",
	"license": "MIT",
	"repository": "sindresorhus/active-win",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "xo && ava",
		"build-win32": "csc native/win32/*.cs -Out:bin/active-win-win32.exe",
		"build-linux": "cp native/linux/main.js bin/active-win-linux.js",
		"compile-darwin": "swift build --package-path=native/darwin --configuration=release --static-swift-stdlib",
		"move-darwin": "mv native/darwin/.build/release/active-win bin/active-win-darwin",
		"build-darwin": "npm run compile-darwin && npm run move-darwin",
		"prepublishOnly": "npm run build-linux && npm run build-darwin && npm run build-win32"
	},
	"files": [
		"index.js",
		"index.d.ts",
		"bin"
	],
	"keywords": [
		"macos",
		"linux",
		"windows",
		"app",
		"application",
		"window",
		"win",
		"active",
		"focused",
		"current",
		"title",
		"name",
		"id",
		"pid",
		"screenshot",
		"capture",
		"metadata",
		"bounds",
		"memory",
		"usage",
		"bundleid"
	],
	"devDependencies": {
		"ava": "^0.25.0",
		"xo": "^0.23.0"
	}
}