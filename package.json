{
	"name": "active-win",
	"version": "4.0.0",
	"description": "Get metadata about the active window (title, id, bounds, owner, etc). Works on macOS, Linux, Windows.",
	"license": "MIT",
	"repository": "sindresorhus/active-win",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "xo && ava",
		"build-win32": "csc Sources/active-win/main.cs && mv main.exe active-win-win32.exe",
		"build-osx": "swift build --configuration=release --static-swift-stdlib && mv .build/release/active-win active-win-osx",
		"prepublishOnly": "npm run build-osx build-win32"
	},
	"files": [
		"index.js",
		"lib",
		"main"
	],
	"keywords": [
		"macos",
		"linux",
		"windows",
		"app",
		"application",
		"window",
		"win",
		"active",
		"focused",
		"current",
		"title",
		"name",
		"id",
		"pid",
		"screenshot",
		"capture",
		"metadata",
		"bounds",
		"memory",
		"usage",
		"bundleid"
	],
	"devDependencies": {
		"ava": "^0.25.0",
		"xo": "^0.23.0"
	}
}
