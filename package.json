{
	"name": "@arcsine/active-win",
	"version": "5.0.3",
	"description": "Get metadata about the active window (title, id, bounds, screens, owner, etc). Works on macOS, X11-based Desktops (Linux, BSD), Windows.",
	"license": "MIT",
	"repository": "arciisine/active-win",
	"author": {
		"name": "Timothy Soehnlin",
		"email": "timothy.soehnlin@gmail.com",
		"url": "github.com/arciisine"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"test": "ava",
		"build-win32": "csc native/win32/*.cs -Out:bin/active-win-win32.exe",
		"build-x11": "cp native/x11/main.js bin/active-win-x11.js",
		"compile-darwin": "swift build --package-path=native/darwin --configuration=release --static-swift-stdlib",
		"move-darwin": "mv native/darwin/.build/release/active-win bin/active-win-darwin",
		"build-darwin": "npm run compile-darwin && npm run move-darwin",
		"compile": "mkdir bin; npm run build-x11 && npm run build-darwin && npm run build-win32",
		"prepublish": "npm run compile"
	},
	"maintainers": [
		{
			"name": "Timothy Soehnlin",
			"url": "https://github.com/arciisine"
		},
		{
			"name": "Sindre Sorhus",
			"url": "https://github.com/sindresorhus"
		},
		{
			"name": "Sebastián Ramírez",
			"url": "https://github.com/tiangolo"
		}
	],
	"files": [
		"index.js",
		"index.d.ts",
		"bin"
	],
	"keywords": [
		"macos",
		"linux",
		"x11",
		"windows",
		"app",
		"application",
		"window",
		"win",
		"active",
		"focused",
		"current",
		"title",
		"name",
		"id",
		"pid",
		"screenshot",
		"capture",
		"metadata",
		"bounds",
		"memory",
		"usage",
		"bundleid"
	],
	"devDependencies": {
		"ava": "^0.25.0"
	}
}